<Remote_Control>:<MyButton@Button>:    canvas:        Color:            rgba: .686,.686,.686,.09        RoundedRectangle:            segments: 1            radius: (5,(60,100),5,(60,100))            size: self.size            pos: self.pos            GridLayout:    id: full_screen    cols: 2    GridLayout:        cols: 1        size_hint_y: 1        size_hint_x: None        width: 300        Label:            font_size: '24sp'            bold: True            color: 1,1,0,1             text: 'Goddard Rover Control'            size_hint_y: .5            # Main Button Panel starts here#     CAMERA GIMBAL        GridLayout:            cols: 3            size_hint_y: 2            Button:                id: calibrate_camera                background_normal: '/home/app/buttons/camera_calibrate.png'                on_press: app.calibrate_camera()            Button:                id: camera_tilt_up                border: 0, 0, 0, 0                background_normal: '/home/app/buttons/camera_up.png'                on_press: app.camera_tilt_up()            Button:                id: camera_180_pan                border: 0, 0, 0, 0                background_normal: '/home/app/buttons/camera_180.png'                on_press: app.camera_180_pan()            Button:                id: camera_pan_left                border: 8, 8, 8, 8                background_normal: '/home/app/buttons/camera_left.png'                on_press: app.camera_pan_left()            Button:                id: camera_home                border: 0, 0, 0, 0                background_normal: '/home/app/buttons/camera_home.png'                on_press: app.camera_home()            Button:                id: camera_pan_right                border: 8, 8, 8, 8                background_normal: '/home/app/buttons/camera_right.png'                on_press: app.camera_pan_right()            Button:                id: camera_90_pan_left                border: 0, 0, 0, 0                background_normal: '/home/app/buttons/camera_90_left.png'                on_press: app.camera_90_pan_left()            Button:                id: camera_tilt_down                border: 0, 0, 0, 0                background_normal: '/home/app/buttons/camera_down.png'                on_press: app.camera_tilt_down()            Button:                id: camera_90_pan_right                border: 0, 0, 0, 0                background_normal: '/home/app/buttons/camera_90_right.png'                on_press: app.camera_90_pan_right()#     WHEEL CONTROLS        GridLayout:            cols: 1            size_hint_y: 3            Slider:                id: slider_speed                max: 50                step: 5                value: 0                valign: 'top'                size_hint_y: 1                on_value: app.slider_speed()            GridLayout:                cols: 3                size_hint_y: .5                Button:                    text: 'Slow Speed'                    on_press: slider_speed.value = 20                    on_press: direction.text = 'Slow Forward'                Button:                    text: 'Mid-Speed'                    on_press: slider_speed.value = 35                    on_press: direction.text = 'Medium Forward'                Button:                    text: 'Full Speed'                    on_press: slider_speed.value = 50                    on_press: direction.text = 'Full Forward'            GridLayout:                cols: 3                size_hint_y: .5                Label:                    text: "Speed is: " + str(int(slider_speed.value))                Label:                    bold: True                    color: 1,1,0,1                     text: "Wheels"                Label:                    id: direction                    text: 'Forward'                      GridLayout:                cols: 3                size_hint_y: 3                GridLayout:                    cols: 1                    Label:                        id: temperature                        color: 0,1,1,1                        text: 'Temp: ??Â° F'                                  Label:                        id: battery_capacity                        text:  'Battery: ??%'                Button:                    id: motor_forward                    border: 0, 0, 0, 0                    background_normal: '/home/app/buttons/wheels_up.png'                    on_press: direction.text = 'Forward'                    on_press: app.wheels_forward()                Button:                    id: wheel_180_turn                    border: 0, 0, 0, 0                    background_normal: '/home/app/buttons/wheels_180.png'                    on_press: app.wheel_180_turn()                Button:                    id: left_motor_forward                    border: 0, 0, 0, 0                    background_normal: '/home/app/buttons/wheels_left.png'                    on_press: direction.text = 'Turning Left'                    on_press: app.wheels_left()                    on_release: direction.text = 'Forward'                Button:                    id: motor_stop                    border: 0, 0, 0, 0                    background_normal: '/home/app/buttons/wheels_stop.png'                    on_press: direction.text = 'Stopped'                    on_press: slider_speed.value = 0                Button:                    id: right_motor_forward                    border: 0, 0, 0, 0                    background_normal: '/home/app/buttons/wheels_right.png'                    on_press: direction.text = 'Turning Right'                    on_press: app.wheels_right()                    on_release: direction.text = 'Forward'                Button:                    id: wheel_90_turn_left                    border: 0, 0, 0, 0                    background_normal: '/home/app/buttons/wheels_45_left.png'                    on_press: app.wheel_90_turn_left()                Button:                    id: motor_reverse                    border: 0, 0, 0, 0                    background_normal: '/home/app/buttons/wheels_down.png'                    on_press: direction.text = 'Reverse'                    on_press: app.wheels_reverse()                                    Button:                    id: wheel_90_turn_right                    border: 0, 0, 0, 0                    background_normal: '/home/app/buttons/wheels_45_right.png'                    on_press: app.wheel_90_turn_right()    GridLayout:        cols: 1        size_hint_x: None        width: 500        BoxLayout:            cols: 10            orientation: 'horizontal'            height: 48            size_hint_y: None            Label:                text: ' '            Label:                text: ' '            Label:                text: ' '            Label:                text: ' '            Label:                text: ' '            Label:                text: ' '            Label:                text: ' '            Button:                id: batteries_icon                background_normal: '/home/app/icons/Rfull_RCfull.PNG'                on_press: app.app_reboot()            Button:                id: wifi_icon                background_normal: '/home/app/icons/Wi-FiIcon-full.PNG'            Button:                id: rover_icon                background_normal: '/home/app/icons/RoverIconGreen.PNG'                on_press: app.remote_shutdown()        Video:            id: videoid            height: 281            state: 'play'            source: "rtsp://10.10.10.100:8059/live/picam"        BoxLayout:            cols: 7            orientation: 'horizontal'            height: 64            size_hint_y: None            Label:                text: ' '            Label:                text: ' '            ToggleButton:                id: video_record                background_normal: '/home/app/buttons/VideoRecordButton.png'                on_press: self.background_normal = '/home/app/buttons/VideoStopButton.png'                on_press: app.video_record()                state: 'normal'            Button:                background_normal: '/home/app/buttons/VideoSnapshotButton.png'#                on_press: videoid.export_to_png('/home/app/video_snapshot.png')                on_release: app.video_snapshot()            Button:                background_normal: '/home/app/buttons/ScreenSnapshotButton.png'                on_press: full_screen.export_to_png('/home/app/screen_snapshot.png')                on_release: app.screen_snapshot()            Label:                text: ' '            Label:                text: ' '